{
    "name": "jules/zip2text-php",
    "description": "A PHP web application that extracts text from images within a ZIP file using Google Cloud Vision API, with real-time progress updates.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jules",
            "email": "jules@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "google/cloud-vision": "^1.0",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Zip2Text\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zip2Text\\Tests\\": "tests/"
        }
    }
}
